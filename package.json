{
  "name": "parking-db",
  "repository" : {
    "type" : "git",
    "url"  : "http://github.com/RJFryman/parking.git"
  },
  "version": "0.0.1",
  "bin": {"coveralls" : "./node_modules/coveralls/bin/coveralls.js"},
  "private": true,
  "scripts": {
    "start"     : "node app/app.js",
    "test"      : "mocha --recursive test",
    "testwatch" : "mocha -w --recursive test",
    "coveralls" : "./node_modules/mocha/bin/mocha test --recursive -r blanket -R mocha-lcov-reporter | coveralls"
  },
  "dependencies": {
    "express":         "3.4.8",
    "lodash":          "2.9.1",
    "moment":          "2.5.1",
    "mongodb":         "1.3.23",
    "bcrypt":          "0.7.7",
    "connect-redis":   "1.5",
    "jade":            "1.3.0",
    "express-less":    "0.0.2",
    "request":         "2.34.0",
    "express-session": "1.0.2"
  },
  "devDependencies": {
    "grunt":                "*",
    "grunt-contrib-jshint": "*",
    "jshint-stylish":       "*",
    "grunt-contrib-watch":  "*",
    "mocha":                "*",
    "chai" :                "*",
    "superagent":           "*",
    "supertest":            "*",
    "coveralls":            "*",
    "mocha-lcov-reporter":  "*",
    "blanket":              "*"
  },
  "config": {
    "blanket": {
      "pattern": "/app/"
    }
  }
}

